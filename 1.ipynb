    "# check if string is binary string or not.\n",
    "def check(string) : \n",
    "\n",
    "    p = set(string) \n",
    "    s = {'0', '1'} \n",
    "\n",
    "    if s == p or p == {'0'} or p == {'1'}: \n",
    "        print(\"Yes\") \n",
    "    else : \n",
    "        print(\"No\") \n",
    "\n",
    "if __name__ == \"__main__\" :  \n",
    "    string = \"101010000111\"\n",
    "    check(string) "
   ]
  }
